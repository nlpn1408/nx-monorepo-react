{
  "name": "shop-dashboard",
  "projectType": "application",
  "sourceRoot": "apps/shop-dashboard/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/shop-dashboard"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "shop-dashboard:build",
        "port": 4200
      }
    }
  }
}
