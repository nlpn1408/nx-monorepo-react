{
  "name": "admin-console",
  "projectType": "application",
  "sourceRoot": "apps/admin-console/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/admin-console"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "admin-console:build",
        "port": 4201
      }
    }
  }
}
